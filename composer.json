{
  "require": {
    "aelvan/imager": "v2.1.7",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/cms": "^3.0.0",
    "craftcms/contact-form": "2.2.3",
    "craftcms/contact-form-honeypot": "1.0.2",
    "craftcms/redactor": "2.3.2",
    "dolphiq/sitemap": "1.0.9",
    "nystudio107/craft-minify": "1.2.9",
    "rias/craft-contact-form-extensions": "1.1.3",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
