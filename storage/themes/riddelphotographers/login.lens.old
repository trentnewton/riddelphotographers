<koken:include file="partials/login-header.html" />
		<main class="row align-center" itemscope itemprop="mainContentOfPage" itemtype="http://schema.org/WebPageElement">
			<div class="medium-6 columns">
				<koken:form class="login-form">
					<h1 itemprop="headline"><svg class="icon icon-key"><use xlink:href="#icon-key"></use></svg>&nbsp;Login</h1>
					<koken:not empty="messages.koken_password_error">
						<p class="error">
							{{ messages.koken_password_error }}
						</p>
					</koken:not>
					<p itemprop="text">
						<input type="password" name="password" autofocus placeholder="Enter password">
					</p>
					<button type="submit" class="large button" disabled="disabled">Go</button>
				</koken:form>
			</div>
		</main>
		<koken:asset file="assets/js/min/app-min.js" />
	</body>
</html>